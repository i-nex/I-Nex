#: ../pastebinit:172
msgid "Optional arguments (not supported by all pastebins):"
msgstr ""
"Opcjonalne argumenty (nie wspierane przez wszystkie serwisy typu pastebin)"

#: ../pastebinit:173
msgid "\t-a <author:default is '%s'>"
msgstr "\t-a <autor: domyślny to '%s'>"

#: ../pastebinit:174
msgid "\t-b <pastebin url:default is '%s'>"
msgstr "\t-b <url pastebin: domyślny to '%s'>"

#: ../pastebinit:175
msgid "\t-f <format of paste:default is '%s'>"
msgstr "\t-f <format wklejania: domyślny to '%s'>"

#: ../pastebinit:176
msgid "\t-h This help screen"
msgstr "\t-h To okno pomocy"

#: ../pastebinit:177
msgid "\t-i <input file>"
msgstr "\t-i <plik wejściowy>"

#: ../pastebinit:178
msgid "\t-l List all supported pastebins"
msgstr ""

#: ../pastebinit:179
msgid "\t-j <jabberid for notifications:default is '%s'>"
msgstr "\t-j <ID jabbera dla zgłoszeń: domyślne to '%s'>"

#: ../pastebinit:180
msgid "\t-m <permatag for all versions of a post:default is blank>"
msgstr "\t-m <permatag for all versions of a post:default is blank>"

#: ../pastebinit:181
msgid "\t-r <parent posts ID:defaults to none>"
msgstr "\t-fr<nadrzędne ID wpisów: domyślne to none>"

#: ../pastebinit:182
msgid "\t-t <title of paste:default is blank>"
msgstr "\t-m <tytuł wklejenia: domyślne to blank>"

#: ../pastebinit:183
msgid "\t-u <username> -p <password>"
msgstr "\t-u <nazwa użytkownika> -p <hasło>"

