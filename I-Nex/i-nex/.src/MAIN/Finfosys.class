  ComboBox1.List = Dir("/sys/devices/system/cpu/", "cpu[0-9]")
  'Locate kernels
  MaxCount = 0
  ' Try data = Shell "ls /boot | grep 'vmlinuz*'" For Input
  ' 
  ' While Not Eof(data)
  '       Line Input #data, sLine
  '       MaxCount += 1
  '       ListBox1.Add(sLine, MaxCount)
